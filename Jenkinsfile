pipeline {
    agent any

    stages {
        stage('Build') {
            steps {
                sh 'echo "Build the project..."'
                
            }
        }
        stage('Test') {
            steps {
                sh 'echo "Running tests..."'
                
            }
        }
        stage('Deploy') {
            steps {
                sh 'echo "Deploying the project..."'
                
            }
        }
    }

   
}
